{
	"compilerOptions": {
		"module": "none",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"outFile": "build/spine-both.js",
		"sourceMap": true,
		"declaration": true
	},
	"include": [
		"core/src/**/*",
		"canvas/src/**/*",
		"webgl/src/**/*"
	],
	"exclude": [
		"build",
		"player",
		"threejs",
		"webgl/src/Input.ts",
		"webgl/src/LoadingScreen.ts"
	]
}